<link rel="stylesheet" type="text/css" href="/css/signinStyle.css">

<section class="heading"><h1>Hello Stranger Let's Log You In!</h1></section>
    

<section class="form">

    <form action="/user/create-session" method="post">

        <input type="email" name="email" placeholder="Email" required><br><br>
        
        <input type="password" name="password" placeholder="password" required><br><br>
        
        <button type="submit"><i class="fas fa-sign-in-alt"></i></button>
        
        </form>

        <p>

            <a href="/user/auth/google" style="color: white">Google Sign In/SignUp</a>
    
        </p>

</section>
